{
  "name": "Frame Make",
  "version": "0.2.1",
  "manifest_version": 2,
  "description": "Making Frames for Apty",
  "homepage_url": "https://github.com/MisterFishStickz/FrameMaker",
  "icons": {
    "16": "assets/icon/icon16.png",
    "32": "assets/icon/icon32.png",
    "48": "assets/icon/icon48.png",
    "128": "assets/icon/icon128.png"
},
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Frame Maker"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}